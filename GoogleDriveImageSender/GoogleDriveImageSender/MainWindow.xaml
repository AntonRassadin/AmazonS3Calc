<Window x:Class="GoogleDriveImageSender.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoogleDriveImageSender"
        mc:Ignorable="d"
        Title="AmazonS3Calc" SizeToContent="WidthAndHeight" ResizeMode="CanMinimize" HorizontalAlignment="Center" VerticalAlignment="Center">
    <Grid HorizontalAlignment="Left" VerticalAlignment="Top" Height="400" Width="300">
        <StackPanel>
            <TextBox x:Name="outputBox" Margin="10,10,10,0" Padding="0,10,10,10" TextWrapping="Wrap" Text="RESULT" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Background="LightBlue" FontSize="20"/>
            <Button x:Name="exportToAWSButton" Content="export to aws s3" HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top" Width="112" Height="26" RenderTransformOrigin="3.648,0.86" Click="ExportToAWSButton_Click"/>
            <Label Content="" HorizontalAlignment="Right" Margin="0,0,10,0" Foreground="Red" Name="outputInfo"/>
        </StackPanel>
    </Grid>
</Window>
